{
    "name": "prophets/craftplugin-geoaddress",
    "description": "Geo Address fields for Craft Sections",
    "type": "craft-plugin",
    "support": {
        "docs": "https://github.com/Prophets/craftplugin-geoaddress/blob/master/README.md",
        "issues": "https://github.com/Prophet/craftplugin-geoaddress/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Prophets",
            "homepage": "https://www.prophets.be"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "Prophets\\GeoAddress\\": "src/"
        }
    },
    "extra": {
        "name": "Prophets - Geo Address Fields",
        "handle": "geoaddress",
        "schemaVersion": "1.0.1",
        "hasCpSettings": true,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/Prophets/craftplugin-geoaddress/master/CHANGELOG.md",
        "components": {
            "geoAddressService": "Prophets\\GeoAddress\\services\\GeoAddressService"
        },
        "class": "Prophets\\GeoAddress\\GeoAddress"
    }
}
